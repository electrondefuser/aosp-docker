# DO NOT EDIT THIS FILE INSIDE DOCKER

export LC_ALL=C

export USE_CCACHE=1
export CCACHE_DIR=/ccache
export CCACHE_EXEC=/usr/bin/ccache

export JAVA6_HOME=/usr/lib/jvm/jdk1.6.0_45
export JAVA7_HOME=/usr/lib/jvm/java-7-openjdk-amd64
export JAVA8_HOME=/usr/lib/jvm/java-8-openjdk-amd64

__buildpathfromsaved() {
    export PATH=$SAVEDPATH
}

__aosp_parent() {
    export STAY_OFF_MY_LAWN=1
    export PATH=$JAVA_HOME/bin:$PATH
    __buildpathfromsaved
}

usejava6() {
    __aosp_parent
    export JAVA_HOME=$JAVA6_HOME
}

usejava7() {
    __aosp_parent
    export JAVA_HOME=$JAVA7_HOME
}

usejava8() {
    __aosp_parent
    export JAVA_HOME=$JAVA8_HOME
}

if [ -z "$SAVEDPATH" ]; then
    SAVEDPATH=$PATH
fi

__buildpathfromsaved
